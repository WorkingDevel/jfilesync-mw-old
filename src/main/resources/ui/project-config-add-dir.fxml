<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<TitledPane fx:id="rootPane" animated="false" prefWidth="400.0" text="untitled" xmlns="http://javafx.com/javafx/8.0.121"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="jfsmw.ui.javafx.project.ProjectConfigAddDirController">
  <content>
    <GridPane>
      <columnConstraints>
        <ColumnConstraints hgrow="NEVER" minWidth="100.0" prefWidth="100.0"/>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="200.0" prefWidth="200.0"/>
        <ColumnConstraints hgrow="NEVER" minWidth="30.0" prefWidth="30.0"/>
      </columnConstraints>
      <rowConstraints>
        <RowConstraints minHeight="30.0" prefHeight="30.0" vgrow="NEVER"/>
        <RowConstraints minHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES"/>
      </rowConstraints>
      <children>
        <TextField fx:id="textDirectory" GridPane.columnIndex="1"/>
        <Button fx:id="btnSelectDirectory" mnemonicParsing="false" onAction="#openDirectoryAction" text="..."
                GridPane.columnIndex="2"/>
        <Label text="Directory"/>
        <ChoiceBox fx:id="choiceType" prefWidth="150.0" GridPane.columnIndex="1" GridPane.columnSpan="2"
                   GridPane.rowIndex="1"/>
        <Label text="Type" GridPane.rowIndex="1"/>
      </children>
    </GridPane>
  </content>
</TitledPane>
